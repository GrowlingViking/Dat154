<Window x:Class="SolarSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SolarSystem"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="319" VerticalAlignment="Top" Width="76" Background="SeaShell">
            <Button x:Name="button" Content="Start" HorizontalAlignment="Center" Width="76"/>
        </StackPanel>
        <Ellipse Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="50" Margin="170,96,0,0" Stroke="Black" VerticalAlignment="Top" Width="50"/>

    </Grid>
</Window>
